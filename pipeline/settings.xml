    <servers>
		<server>
		  <id>MuleRepository</id>
		  <username>yourMuleNexusID</username>
		  <password>yourMuleNexusPassword</password>
		</server>
	    <server>
	      <id>anypoint-exchange-v3</id>
	      <username>myexchange</username>
	      <password>0209d120726a46928785e5584c7c20a8~?~99e03cd1443040fEaEA8557de99A3C4c</password>
	    </server>
	    <server>
	      <id>Repository</id>
	      <username>repository-mule</username>
